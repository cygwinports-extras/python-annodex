  /usr/lib/python2.5/site-packages/annodex/anx.pyc
  /usr/lib/python2.5/site-packages/annodex/anx.pyo
  /usr/lib/python2.5/site-packages/annodex/anx_types.py
  /usr/lib/python2.5/site-packages/annodex/anx_types.pyc
  /usr/lib/python2.5/site-packages/annodex/anx_types.pyo
