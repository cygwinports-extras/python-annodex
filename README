  /usr/lib/python2.4/site-packages/annodex/anx.py
  /usr/lib/python2.4/site-packages/annodex/anx.pyc
  /usr/lib/python2.4/site-packages/annodex/anx.pyo
  /usr/lib/python2.4/site-packages/annodex/anx_types.py
  /usr/lib/python2.4/site-packages/annodex/anx_types.pyc
  /usr/lib/python2.4/site-packages/annodex/anx_types.pyo
  /usr/share/doc/Cygwin/pyannodex-0.7.3.README
  /usr/share/doc/pyannodex-0.7.3/AUTHORS
  /usr/share/doc/pyannodex-0.7.3/COPYING
